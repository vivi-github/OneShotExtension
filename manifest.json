{
  "name": "Get pages source",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Get pages source from a popup",
  "sidebar_action": {
    "default_icon": "icon.png",
    "default_page": "popup.html"
  },  

  "permissions": [
    "tabs",
    "<all_urls>",
    "cookies",
    "http://*/*",
    "https://*/*",
    "activeTab",
    "https://blog.naver.com/",
    "http://blog.naver.com/",
    "https://www.naver.com/",
    "http://blogattach.naver.net/",
    "downloads"

  ]
}
