{
  "name": "Get pages source",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Get pages source from a popup",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "cookies",
    "http://*/*",
    "https://*/*",
    "activeTab",
    "https://blog.naver.com/",
    "http://blog.naver.com/"
  ]
}
